  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Singlepage Server",
      "program": "${workspaceFolder}\\dev\\dist\\bin\\spstart.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Build Distribution",
      "program": "${workspaceFolder}\\dev\\tools\\build_dist.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Create App",
      "program": "${workspaceFolder}\\bin\\sp-cli.js",
      "cwd": "${workspaceFolder}\\..",
      "args": ["create", "app1"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Build App",
      "program": "${workspaceFolder}\\bin\\sp-cli.js",
      "cwd": "${workspaceFolder}\\..\\app1",
      "args": ["build"]
    }
  ]
}